git config --global user.name "username"
git config --global user.email "email"
git config --list
git help config = git config --help
git init
git status
.gitignore
    .DS_Store
    *.txt
git add -A
git reset // all the file
git commit -m "Initial Commit"
git log
git clone url where_to_clone
git remote -v
git branch -a
git diff
